Geometry = GenFormat {
   <<< "geo.gen"
}
Driver = {}
Hamiltonian {
  DFTB {
    SCC = Yes
    SCCTolerance = 1e-8
    MaxSCCIterations = 1000
    Charge = 0.0
    MaxAngularMomentum {
      C = "p"
      H = "s"
    }
    SlaterKosterFiles {
      Type2FileNames {
        Prefix = "slakos/origin/mio-1-1/"
        Separator = "-"
        Suffix = ".skf"
      }
    }
  }
}

ExcitedState {
  Casida {
    TammDancoff=Yes
    NrOfExcitations = 10
    StateOfInterest = 1
    Symmetry = Singlet
    Diagonalizer {
      Arpack {}
    }
  }
}

ParserOptions {
  ParserVersion = 13
}
Parallel {
  UseOmpThreads = Yes
}
Options {
  WriteAutotestTag = Yes
}
